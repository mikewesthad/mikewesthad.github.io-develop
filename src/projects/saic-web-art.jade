extend ../jade-layouts/_project-page.jade

block variables
    -
        var projectName = "saic-web-art";
        var projectImagePath = "/images/projects/" + projectName + "/";
        var projectTitle = "";
        var projectHeadline = "Web Art at SAIC";
        var projectDetails = [
            "2016 - Present",
            "Tags: Teaching"
        ];

block metaOpenGraph
    - const ogTitle = "I taught Web Art at SAIC";
    - const ogDescription = "";
    - const ogImage = `https://www.mikewesthad.com/images/projects/${projectName}/google-frames.jpg`;
    meta(property="og:type" content="website")
    meta(property="og:url" content=`https://www.mikewesthad.com/${projectName}/`)
    meta(property="og:image" content=ogImage)
    meta(property="og:title" content=ogTitle)
    meta(property="og:description" content=ogDescription)

block projectDescription
    +titledSection("Overview")
        p I teach Web Art at SAIC. See the most recent #[a(href="https://www.mikewesthad.com/saic-webart/") class website].